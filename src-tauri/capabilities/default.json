{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables: default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:window:allow-maximize",
    "core:window:allow-minimize",
    "core:window:allow-close",
    "core:window:allow-unmaximize",
    "core:window:allow-start-dragging",
    "shell:allow-open",
    "shell:default",
    "dialog:allow-save",
    "dialog:default",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:default"
  ]
}
